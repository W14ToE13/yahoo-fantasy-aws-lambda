{
  "configurations": [
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "Task",
      "invokeTarget": {
        "target": "template",
        "templatePath": "${workspaceFolder}/template.yaml",
        "logicalId": "LongRunningJobFunction"
      },
      "lambda": {
        "runtime": "python3.11", // Add the runtime property
        "payload": {
          "json": {
            "sessionId": "sdfsfasfasfas",
            "league_key": "428.l.7124",
            "league_id": "7124",
            "week": "1"        
          }
        }
      }
    },
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "Login",
      "invokeTarget": {
        "target": "template",
        "templatePath": "${workspaceFolder}/template.yaml",
        "logicalId": "WebServerFunction"
      },
      "lambda": {
        "runtime": "python3.11",
        "payload": {
          "json": {
            "rawPath": "/login",
            "cookies": ["sessionId=example-session-id"]
          }
        }
      }
    },
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "Analze",
      "invokeTarget": {
        "target": "api",
        "templatePath": "${workspaceFolder}/template.yaml",
        "logicalId": "WebServerFunction"
      },
      "api": {
        "path": "/api/analyze",
        "httpMethod": "post",
        "payload": {
          "json": {
            "league_key": "428.l.7124",
            "league_id": "7124",
            "week": "1"
          }
        },
        "querystring": "abc=def&qrs=tuv",
        "headers": {
          "cookie": "sessionId=ca6c34ce-778a-46b5-b4ad-dc40ab7167d5; name2=value2; name3=value3"
        }
      }
    },
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "leauges",
      "invokeTarget": {
        "target": "api",
        "templatePath": "${workspaceFolder}/template.yaml",
        "logicalId": "WebServerFunction"
      },
      "api": {
        "path": "/api/leagues",
        "httpMethod": "get",

        "querystring": "abc=def&qrs=tuv",
        "headers": {
          "cookie": "sessionId=8c3b424d-cef1-4a0f-a3fa-bdcfdd989cc4; name2=value2; name3=value3"
        }
      }
    }  
  ]
}
